server {
  listen 443 ssl;

  server_name dashboard.zuul.ansible.com;

  # RSA certificate
  ssl_certificate /etc/letsencrypt/live/dashboard.zuul.ansible.com/fullchain.pem;
  ssl_certificate_key /etc/letsencrypt/live/dashboard.zuul.ansible.com/privkey.pem;

  location / {
    proxy_pass http://localhost:9000/;
    proxy_http_version 1.1;
    proxy_set_header Upgrade $http_upgrade;
    proxy_set_header Connection "upgrade";
  }
}
